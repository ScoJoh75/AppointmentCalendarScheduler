<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="190.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewcontroller.LoginScreen">
    <TextField fx:id="loginUN" layoutX="140.0" layoutY="56.0"/>
    <PasswordField fx:id="loginPW" layoutX="140.0" layoutY="88.0"/>
    <Button fx:id="loginSubmit" alignment="CENTER" contentDisplay="CENTER" layoutX="196.0" layoutY="142.0"
            mnemonicParsing="false" onAction="#processLogin" prefHeight="25.0" prefWidth="99.0" text="Log In"
            textAlignment="CENTER"/>
    <Label fx:id="loginLabel" alignment="CENTER_RIGHT" layoutX="18.0" layoutY="60.0" prefHeight="17.0" prefWidth="112.0"
           text="Username:" textAlignment="RIGHT"/>
    <Label fx:id="passwordLabel" alignment="CENTER_RIGHT" layoutX="18.0" layoutY="93.0" prefHeight="17.0"
           prefWidth="112.0" text="Password:" textAlignment="RIGHT"/>
</AnchorPane>
